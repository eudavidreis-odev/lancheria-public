# Exemplo de variáveis de ambiente — copie para `.env` localmente e preencha.
# NÃO comite o arquivo `.env` real.

# Firebase (preencha com valores do seu projeto)
FIREBASE_API_KEY=
FIREBASE_AUTH_DOMAIN=
FIREBASE_PROJECT_ID=
FIREBASE_APP_ID=
FIREBASE_MESSAGING_SENDER_ID=
FIREBASE_STORAGE_BUCKET=
FIREBASE_MEASUREMENT_ID=

# Arquivos nativos (caso use builds nativos). Coloque os ficheiros nas paths locais
# indicadas, NÃO os comite.
ANDROID_GOOGLE_SERVICES_FILE=./google-services.json
IOS_GOOGLE_SERVICES_FILE=./GoogleService-Info.plist

# Ambiente
NODE_ENV=development
